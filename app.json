{
  "expo": {
    "name": "Price Drop",
    "slug": "PriceDrop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/pdappicon3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/pdappsplashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#e32f45"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pricedrop.pricedrop",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/pdappicon3.png",
        "backgroundColor": "#e32f45"
      }
    },
    "web": {
      "favicon": "./assets/icons/pdappicon3.png"
    }
  }
}
